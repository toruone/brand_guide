%nav.side-nav.side-nav-left
  %h2
    -# %a{:href => "/"} World Brands+
    =link_to top_path do
      %img{:alt => "", :src => "/img/SiteLogo6.png"}/
  %ul.list-unstyled
    -if user_signed_in?
      %li
        =link_to "Brands", brands_path
      %li
        =link_to "Info Sites", info_sites_path
      %li
        = link_to 'My Page', login_mypage_path
      %li
        = link_to 'My Profile', edit_login_mypage_profile_path
      %li
        = link_to 'Log Out', destroy_user_session_path
      %li
        = link_to 'About Us', about_path
      %li
        = link_to 'Contact Us', new_contact_path
    -else
      %li
        =link_to "Brands", brands_path
      %li
        =link_to "Info Sites", info_sites_path
      %li
        = link_to 'Login', new_user_session_path
      %li
        = link_to 'Sign Up', new_user_registration_path
      %li
        = link_to 'About Us', about_path
      %li
        = link_to 'Contact Us', new_contact_path

.site-overlay
%header
  .container-fluid
    .row
      .col-sm-12
        .menu.pull-left{"data-placement" => "bottom", "data-toggle" => "tooltip", :title => "Menu"}
          -# %i.icon.ion-ios-more
          %img{:alt => "", :src => "/img/SiteLogo6.png"}/
        .account.pull-right
          %ul.list-inline
            %li.dropdown
              %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
                %i.icon.ion-ios-person-outline{"data-placement" => "bottom", "data-toggle" => "tooltip", :title => "My Account"}
              %ul.dropdown-menu.dropdown-menu-right.text-center
                %li.dropdown-triangle
                -# -if admin_signed_in?
                -#   %li
                -#     = link_to 'Log Out(admin)', destroy_admin_session_path
                -if user_signed_in?
                  %li
                    = link_to 'My Page', login_mypage_path
                  %li
                    = link_to 'My Profile', edit_login_mypage_profile_path
                  %li
                    = link_to 'Log Out', destroy_user_session_path
                -else
                  %li
                    =link_to "Log In", new_user_session_path
                  %li
                    =link_to "Sign Up", new_user_registration_path

